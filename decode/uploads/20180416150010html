    <div class="ficha-spcultura">
        
        <p>
            <span class="js-editable required editable editable-pre-wrapped editable-click" data-edit="shortDescription" data-original-title="Descrição Curta" data-emptytext="Insira uma descrição curta" data-tpl="&lt;textarea maxlength=&quot;400&quot;&gt;&lt;/textarea&gt;">Escola Livre de Dança abre inscrições para DISCIPLINAS ESPECÍFICAS e para os LABORATÓRIOS DE PESQUISA EM DANÇA</span>
        </p>
                <div class="servico">
                                        <p>
                    <span class="label ">Site:</span>
                    <!-- ngIf: data.isEditable --><span ng-if="data.isEditable" class="js-editable ng-scope editable editable-click editable-empty" data-edit="site" data-original-title="Site" data-emptytext="Insira a url de seu site">Insira a url de seu site</span><!-- end ngIf: data.isEditable -->
                    <!-- ngIf: !data.isEditable -->
                </p>
